<configuration>
  <connectionStrings>
    <add name="exam" connectionString="Data Source=192.168.1.139;Initial Catalog=exam;Persist Security Info=True;User ID=sa;Password=sa" providerName="System.Data.SqlClient"/>
  </connectionStrings>
  <appSettings>
    <add key="userpath" value="~/userpath/"/>
  </appSettings>
    <system.web>
      <authentication mode="Forms">
        <forms name=".EXAM_USER_T" loginUrl="~/Login.aspx" protection="All" path="/"/>
      </authentication>
      <authorization>
        <deny users="?"/>
      </authorization>

      <compilation debug="true" strict="false" explicit="true" targetFramework="4.0" />
    </system.web>
  <location path="Logout.aspx">
    <system.web>
      <authorization>
        <allow users="?"/>
      </authorization>
    </system.web>
  </location>
  <location path="css">
    <system.web>
      <authorization>
        <allow users="?"/>
      </authorization>
    </system.web>
  </location>
  <location path="image">
    <system.web>
      <authorization>
        <allow users="?"/>
      </authorization>
    </system.web>
  </location>
  <location path="js">
    <system.web>
      <authorization>
        <allow users="?"/>
      </authorization>
    </system.web>
  </location>
</configuration>
